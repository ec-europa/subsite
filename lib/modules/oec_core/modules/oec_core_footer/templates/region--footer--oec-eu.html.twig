{# EU Footer #}

{#
Some external links doesnt have page in appropriate language. In this case we show these links in English.
Some links compoatible only with predefined list of lanuguages.
#}
{% set langcode_en_fr = langcode not in ['fr'] ? 'en' : langcode %}
{% set langcode_en_fr_de = langcode not in ['fr', 'de'] ? 'en' : langcode %}

<footer class="ecl-footer ecl-footer--updated ecl-footer--custom">
  <section class="ecl-footer__identity">
    <div class="ecl-container">
      <h1 class="ecl-footer__identity-title">{{ site_name }}</h1>
      <div class="ecl-row">
        <div class="ecl-footer__identity-follow ecl-col-12 ecl-col-md-6"><span class="ecl-footer__identity-label">{{ 'Follow us'|t }}:</span>
          <a href="{{ social_media_links.facebook }}" class="ecl-footer__identity-link ecl-link ecl-link--standalone ecl-link--icon ecl-link--icon-before" aria-label="Link to Facebook">
            <svg focusable="false" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="branded--facebook" class="ecl-link__icon ecl-icon ecl-icon--xs"><path d="M24 12.073C24 5.405 18.627 0 12 0S0 5.405 0 12.073C0 18.099 4.388 23.094 10.125 24v-8.437H7.078v-3.49h3.047v-2.66c0-3.025 1.792-4.697 4.533-4.697 1.312 0 2.686.236 2.686.236v2.971h-1.513c-1.491 0-1.956.931-1.956 1.886v2.264h3.328l-.532 3.49h-2.796V24C19.612 23.094 24 18.099 24 12.073"></path></svg>&nbsp;<span class="ecl-link__label">Facebook</span>
          </a>

          <a href="{{ social_media_links.twitter }}" class="ecl-footer__identity-link ecl-link ecl-link--standalone ecl-link--icon ecl-link--icon-before" aria-label="Link to Twitter">
            <svg focusable="false" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="branded--twitter" class="ecl-link__icon ecl-icon ecl-icon--xs"><path d="M20.592 8.031C20.592 14.068 16.104 21 7.896 21 5.269 21 2.86 20.217 1 18.876h.985c2.08 0 3.94-.783 5.582-2.013a4.415 4.415 0 01-4.268-3.13c.437.112.656.112.875.112.438 0 .766 0 1.204-.112-2.08-.447-3.612-2.236-3.612-4.472.657.223 1.314.559 2.08.559-1.314-.783-2.08-2.236-2.08-3.801 0-.895.22-1.677.657-2.348 2.189 2.907 5.582 4.695 9.303 4.92-.11-.448-.11-.672-.11-1.007 0-2.46 2.08-4.584 4.488-4.584 1.423 0 2.627.559 3.394 1.453.985-.111 1.97-.559 2.845-1.006-.219 1.006-.985 2.013-1.97 2.46.876-.112 1.751-.447 2.627-.783-.766.783-1.532 1.565-2.408 2.236v.671z"></path></svg>&nbsp;<span class="ecl-link__label">Twitter</span>
          </a>
          <a href="{{ social_media_links.instagram }}" class="ecl-footer__identity-link ecl-link ecl-link--standalone ecl-link--icon ecl-link--icon-before" aria-label="Link to Instagram">
            <svg focusable="false" aria-hidden="true" class="ecl-link__icon ecl-icon ecl-icon--xs" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" id="branded--instagram"><path d="M11.996.008C8.738.008 8.33.021 7.05.08 5.773.138 4.901.341 4.138.637a5.886 5.886 0 00-2.125 1.384A5.883 5.883 0 00.63 4.146c-.297.763-.5 1.635-.558 2.912C.014 8.337 0 8.746 0 12.004s.014 3.666.072 4.946c.058 1.277.261 2.149.558 2.912a5.883 5.883 0 001.383 2.125 5.883 5.883 0 002.125 1.383c.763.297 1.635.5 2.912.558 1.28.058 1.688.072 4.946.072s3.667-.014 4.946-.072c1.277-.058 2.149-.261 2.912-.558a5.883 5.883 0 002.125-1.383 5.886 5.886 0 001.384-2.125c.296-.763.499-1.635.557-2.912.059-1.28.072-1.688.072-4.946s-.013-3.667-.072-4.946c-.058-1.277-.261-2.149-.557-2.912-.307-.789-.717-1.458-1.384-2.125S20.643.944 19.854.637C19.091.341 18.219.138 16.942.08 15.663.021 15.254.008 11.996.008zm0 2.161c3.203 0 3.583.012 4.848.07 1.169.053 1.805.249 2.227.413.56.218.96.478 1.38.897.419.42.679.82.897 1.38.164.422.36 1.058.413 2.227.058 1.265.07 1.645.07 4.848s-.012 3.582-.07 4.847c-.053 1.17-.249 1.805-.413 2.228a3.73 3.73 0 01-.897 1.379c-.42.42-.82.68-1.38.898-.422.164-1.058.359-2.227.413-1.265.057-1.644.07-4.848.07-3.203 0-3.582-.013-4.847-.07-1.17-.054-1.805-.249-2.228-.413-.56-.218-.959-.478-1.379-.898s-.68-.819-.898-1.379c-.164-.423-.359-1.058-.413-2.228-.057-1.265-.07-1.644-.07-4.847s.013-3.583.07-4.848c.054-1.169.249-1.805.413-2.227.218-.56.478-.96.898-1.38a3.73 3.73 0 011.379-.897c.423-.164 1.058-.36 2.228-.413 1.265-.058 1.644-.07 4.847-.07z"></path><path d="M11.996 16.003a4 4 0 11.002-8 4 4 0 01-.002 8zm0-10.159a6.16 6.16 0 100 12.32 6.16 6.16 0 000-12.32zM19.839 5.6a1.44 1.44 0 11-2.879.001 1.44 1.44 0 012.879-.001"></path></svg>&nbsp;<span class="ecl-link__label">Instagram</span>
          </a>
        </div>
        <div class="ecl-footer__identity-info ecl-col-12 ecl-col-md-6">
          {{ footer_menu }}
        </div>
      </div>
    </div>
  </section>
  <div class="ecl-footer__sections">
    <div class="ecl-container">
      <div class="ecl-row">
        <section class="ecl-footer__section ecl-col-12 ecl-col-md-4">
          <h1 class="ecl-footer__section-title">{{ 'Contact the EU'|t }}</h1>
          <ul class="ecl-footer__section-list">
            <li class="ecl-footer__section-item"><span><span>{{ 'Call us'|t }} </span><a href="tel:0080067891011" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Call us on 00 800 6 7 8 9 10 11">00 800 6 7 8 9 10 11</a></span></li>
            <li class="ecl-footer__section-item"><span><span>{{ 'Use other'|t }} </span><a href="https://europa.eu/european-union/contact/call-us_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to telephone options">{{ 'telephone options'|t }}</a></span></li>
            <li class="ecl-footer__section-item"><span><span>{{ 'Write to us via our'|t }} </span><a href="https://europa.eu/european-union/contact/write-to-us_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to contact form">{{ 'contact form'|t }}</a></span></li>
            <li class="ecl-footer__section-item"><span><span>{{ 'Meet us at a'|t }} </span><a href="https://europa.eu/european-union/contact/meet-us_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to local EU Office">{{ 'local EU office'|t }}</a></span></li>
          </ul>
          <h1 class="ecl-footer__section-title">{{ 'Find an EU social media account'|t }}</h1>
          <ul class="ecl-footer__section-list">
            <li class="ecl-footer__section-item"><span><a href="https://europa.eu/european-union/contact/social-networks_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Search for EU social media channels">{{ 'Search for EU social media channels'|t }}</a></span></li>
          </ul>
        </section>
        <section class="ecl-footer__section ecl-col-12 ecl-col-md-8">
          <h1 class="ecl-footer__section-title">{{ 'EU institution'|t }}</h1>
          <ul class="ecl-footer__section-list ecl-footer__section-list--long ecl-footer__columns">
            <li class="ecl-footer__section-item"><a href="https://www.europarl.europa.eu/portal/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Parliament">{{ 'European Parliament'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.europarl.europa.eu/portal/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Council">{{ 'European Council'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.consilium.europa.eu/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Council of the European Union">{{ 'Council of the European Union'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://ec.europa.eu/info/index_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Commission">{{ 'European Commission'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://curia.europa.eu/jcms/jcms/j_6/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Court of Justice of the European Union">{{ 'Court of Justice of the European Union'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.ecb.europa.eu/home/html/index.{{ langcode }}.html" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Central Bank">{{ 'European Central Bank'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.eca.europa.eu/{{ langcode }}/Pages/ecadefault.aspx" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Court of Auditors">{{ 'European Court of Auditors'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://eeas.europa.eu/headquarters/headquarters-homepage_{{ langcode_en_fr_de }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European External Action Service">{{ 'European External Action Service'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.eesc.europa.eu/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Economic and social Committee">{{ 'European Economic and Social Committee'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://cor.europa.eu/{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Committee of the Regions">{{ 'European Committee of the Regions'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.eib.org/{{ langcode_en_fr_de }}/index.htm" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Investment Bank">{{ 'European Investment Bank'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://www.ombudsman.europa.eu/{{ langcode }}/home" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Ombudsman">{{ 'European Ombudsman'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://edps.europa.eu/edps-homepage_{{ langcode_en_fr_de }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Data Protection Supervisor">{{ 'European Data Protection Supervisor'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://epso.europa.eu/home_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to European Personnel Selection office">{{ 'European Personnel Selection Office'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://op.europa.eu/{{ langcode }}/home" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Publications office of the European Union">{{ 'Publications Office of the European Union'|t }}</a></li>
            <li class="ecl-footer__section-item"><a href="https://europa.eu/european-union/about-eu/agencies_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to Agencies">{{ 'Agencies'|t }}</a></li>
          </ul>
          <ul class="ecl-footer__section-list ecl-footer__section-list--short">
            <li class="ecl-footer__section-item"><span><span>{{ 'Search for'|t }} </span><a href="https://europa.eu/european-union/about-eu/institutions-bodies_{{ langcode }}" class="ecl-footer__section-link ecl-link ecl-link--standalone" aria-label="Link to EU institutions">{{ 'EU institutions'|t }}</a></span></li>
          </ul>
        </section>
      </div>
    </div>
  </div>
  <div class="ecl-footer__common">
    <div class="ecl-container ecl-footer__common-container">
      <a href="https://europa.eu/european-union/about-eu/working_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to Work for the EU">{{ 'Work for the EU'|t }}</a>
      <a href="https://europa.eu/european-union/abouteuropa/sitemap_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to Sitemap">{{ 'Sitemap'|t }}</a>
      <a href="https://europa.eu/european-union/abouteuropa_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to About this Site">{{ 'About this site'|t }}</a>
      <a href="https://europa.eu/european-union/abouteuropa/language-policy_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to Language policy">{{ 'Language policy'|t }}</a>
      <a href="https://europa.eu/european-union/abouteuropa/legal_notices_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to Legal notice">{{ 'Legal notice'|t }}</a>
      <a href="https://europa.eu/european-union/abouteuropa/cookies_{{ langcode }}" class="ecl-footer__common-link ecl-link ecl-link--standalone" aria-label="Link to Cookies">{{ 'Cookies'|t }}</a>
    </div>
  </div>
</footer>
